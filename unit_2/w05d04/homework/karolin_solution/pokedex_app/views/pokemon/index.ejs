<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Pokedex</title>
     <link href="https://fonts.googleapis.com/css?family=Josefin+Sans" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="/normalize.css">
    <link rel="stylesheet" type="text/css" href="/main.css">
  </head>
  <body>
    <div id="container">
      <h1><a href="/pokemon"> Welcome to the Pokedex!</a></h1>
      <h2><a href="/pokemon/new">Make a new pokemon! </a></h2>
      <div id="pokemon-images">
          <% pokemon.forEach(function(onePokemon) { %>
            <div class="card">
              <a href="/pokemon/<%=onePokemon.id %>">
                <img src="<%= onePokemon.img %>" alt="<%= pokemon.name  %>">
                <h3> <%=onePokemon.name %></h3>
              </a>
            </div>
          <% }); %>
        </div>
      </div>
    </div>
  </body>
</html>
